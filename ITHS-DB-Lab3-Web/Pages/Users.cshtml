@page
@model ITHS_DB_Lab3_Web.Pages.UsersModel
@{
    ViewData["Title"] = "Users";
}



<h1>User list</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Role ID</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            @foreach (var UserDetails in Model.userData)
            {
                <partial name="./Shared/_UserSummary" model="UserDetails" />
            }
        </tr>
    </tbody>
</table>
